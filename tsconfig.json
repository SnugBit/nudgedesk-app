{
"compilerOptions": {
"target": "ES2020",
"lib": ["ES2020", "DOM", "DOM.Iterable"],
"module": "ESNext",
"moduleResolution": "Bundler",
"jsx": "preserve",
"allowJs": false,
"strict": true,
"noEmit": true,
"esModuleInterop": true,
"moduleDetection": "force",
"skipLibCheck": true,
"baseUrl": ".",
"paths": { "@/*": ["./*"] },
"resolveJsonModule": true,
"types": ["node"]
},
"include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"],
"exclude": ["node_modules"]
}
